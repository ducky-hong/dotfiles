alt + shift - j : kwmc window -f prev
alt + shift - k : kwmc window -f next
alt + shift - h : kwmc window -c reduce 0.1 east
alt + shift - l : kwmc window -c expand 0.1 east
alt + shift - return : kwmc window -s prev

alt + shift - t : kwmc window -t focused

alt + shift - 1 : kwmc display -f 0
alt + shift - 2 : kwmc display -f 1
alt + shift - q : kwmc window -m display 0
alt + shift - w : kwmc window -m display 1
